{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.apivcs": true,
    "**/.gitignore": true,
    "**/.designer.json": true,
    "**/.exchange.xml": true,
    "**/.vscode": true,
    "**/.mvn": true,
    "**/.tmp": true,
    "**/.settings": true,
    "**/.exchange_modules_tmp": true,
    ".mule": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}